.footer-instagram li.eq-col a{
  display: block;
  position: relative;
}
.footer-instagram li.eq-col a:before{
  content: "";
  position: absolute;
  display: block;
  width: 100%;
  text-align: center;
  content: "\e9a8";
  font-family: "bixbang_icons";
  font-size: 1.3333333333333333em;
  line-height: 1;
  color: #ffffff;
  top: 50%;
  opacity: 0;
  z-index: 7;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
}
.footer-instagram li.eq-col a:after{
  content: "";
  display: block;
  position: absolute;
  z-index: 1;
  left: 0;
  top: 0;
  background-color: rgba(48,48,48,.7);
  height: 100%;
  width: 100%;
  opacity: 0;
  visibility: hidden;
  transform: scale(0);
  -moz-transform: scale(0);
  -webkit-transform: scale(0);
  -moz-transition: 0.2s linear;
  -o-transition: 0.2s linear;
  -webkit-transition: 0.2s linear;
  transition:  0.2s linear;
}
.footer-instagram li.eq-col a:hover:before{
  opacity: 1;
}
.footer-instagram li.eq-col a:hover:after{
  opacity: 1;
  visibility: visible;
  transform: scale(1);
  -moz-transform: scale(1);
  -webkit-transform: scale(1);
}
